# pip install master
git clone https://github.com/pyiron/pyiron_experimental.git ${HOME}/pyiron_experimental
cd ${HOME}/pyiron_experimental
git fetch
git checkout interactive_hs
cd ..

export PYTHONPATH = $PYTHONPATH:${HOME}/pyiron_experimental

# clean up
if [ -d "notebooks" ]; then
    mv notebooks/* .
fi
if [ -d "${HOME}/pyiron_gui_example" ]; then
    rm -r ${HOME}/binder ${HOME}/pyiron_experimental ${HOME}/README.rst
fi
